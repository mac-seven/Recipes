<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the current release version of Quickbooks ODBC Driver.</string>
	<key>Identifier</key>
    	<string>com.github.Mac-Seven.download.QuickbooksODBCDriver2</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Quickbooks ODBC Driver</string>
	</dict>
	<key>Process</key>
	<array>
        <dict>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>Version: ([0-9]+.[0-9]+.[0-9]+)</string>
				<key>result_output_var_name</key>
				<string>VERSIONNUMBER</string>
				<key>url</key>
				<string>https://www.cdata.com/drivers/quickbooks/download/odbc/#macos</string>
                <key>result_output_var_name</key>
                <string>downloadurl</string>
			</dict>
			<key>Processor</key>
			<string>CURLTextSearcher</string>
		</dict>
        <dict>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>(https:\/\/s3.amazonaws.com\/.*.dmg)</string>
				<key>result_output_var_name</key>
				<string>url</string>
				<key>url</key>
				<string>file://%RECIPE_CACHE_DIR%/%NAME%.html</string>
                <key>result_output_var_name</key>
                <string>downloadurl</string>
			</dict>
			<key>Processor</key>
			<string>CURLTextSearcher</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%VERSIONNUMBER%.dmg</string>
				<key>url</key>
				<string>%downloadurl%</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
    </array>
</dict>
</plist>
