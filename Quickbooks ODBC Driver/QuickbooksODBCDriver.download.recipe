<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the current release version of Quickbooks ODBC Driver. 
		This requires the use of an external Python script as the CURLTextSearcher was unable to work with the ampersand in the original download URL.
		Instead a script downloads the html page and then it is referenced locally to get round this issue.</string>
	<key>Identifier</key>
    	<string>com.github.Mac-Seven.download.QuickbooksODBCDriver</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Quickbooks ODBC Driver</string>
	</dict>
	<key>Process</key>
	<array>
        <dict>
            <key>Processor</key>
            <string>SampleSharedProcessor</string>
        </dict>
        <dict>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>Version: ([0-9]+.[0-9]+.[0-9]+)</string>
				<key>url</key>
				<string>https://www.cdata.com/drivers/quickbooks/download/odbc/#macos</string>
                <key>result_output_var_name</key>
                <string>versionnumber</string>
			</dict>
			<key>Processor</key>
			<string>CURLTextSearcher</string>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
    </array>
</dict>
</plist>
